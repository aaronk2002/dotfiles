[mediaplayer]
command=$SCRIPT_DIR/mediaplayer/mediaplayer
instance=spotifyd
interval=1
signal=10

[volume-pulseaudio]
command=$SCRIPT_DIR/volume-pulseaudio/volume-pulseaudio
interval=once
LONG_FORMAT="${SYMB}${VOL}%"
signal=1

[iface]
command=$SCRIPT_DIR/iface/iface
color=#00FF00
interval=10

[time]
command=$SCRIPT_DIR/time/time
interval=1
STRFTIME_FORMAT=%a %m/%d/%Y %H:%M
TZONES=America/New_York,America/Los_Angeles
TZ_LABELS=EST,PST
